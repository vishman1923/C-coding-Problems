#include <stdio.h>
int main()
{
    int row,col,a,b;
    scanf("%d%d",&row,&col);
    int arr[row][col];

    for(int i=0;i<row;i++) 
    {
        for(int j=0;j<col;j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }

    int target,found=0;
    scanf("%d",&target);
    int low=0,mid=0,high=(row*col)-1;

    while(low<=high)
    {
        mid=(low+high)/2;
        a=mid/col;
        b=mid%col;
        if(arr[a][b]==target)
        {
            found=1;
            break;
        }
        else if(target<arr[a][b])
        {
            high=mid-1;
        }
        else if (target>arr[a][b])
        {
            low=mid+1;
        }

    }
    if(found==1)
    {
        printf("target found");
    }
    else
    {
        printf("target not found");
    }
}

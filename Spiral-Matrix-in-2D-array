#include<stdio.h>
int main()
{
    int row,col;
    scanf("%d%d",&row,&col);

    int arr[row][col];

    for(int i=0;i<row;i++) 
    {
        for(int j=0;j<col;j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }

    int t=0,b=row-1,l=0,r=col-1;
    
    while(t<=b && l<=r)
    {
        for(int j=l;j<=r;j++)
        {
            printf("%d ", arr[t][j]);
        }
        t++;

        for(int i=t;i<=b;i++)
        {
            printf("%d ", arr[i][r]);
        }
        r--;

        if(t<=b)
        {
            for(int j=r;j>=l;j--)
            {
                printf("%d ", arr[b][j]);
            }
            b--;
        }

        if(l<=r)
        {
            for(int i=b;i>=t;i--)
            {
                printf("%d ", arr[i][l]);
            }
            l++;
        }
    }
    
    return 0;
}

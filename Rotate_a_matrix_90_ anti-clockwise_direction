#include <stdio.h>
int main() 
{
    int row,col;
    scanf("%d%d",&row,&col);
    int arr[row][col],arr2[row][col]; 
    
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col;j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }
    
    for(int i=0;i<col;i++)
    {
        for(int j=i+1;j<row;j++)
        {
            int temp=arr[i][j];
            arr[i][j]=arr[j][i];
            arr[j][i]=temp;
            }
    } 
    
    for(int i=0;i<col;i++)
    {
        for(int j=0;j<row/2;j++)
        {
            int temp=arr[j][i];
            arr[j][i]=arr[row-1-j][i];
            arr[row-1-j][i]=temp;
        }
    }
    
    
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col;j++)
        {
            printf("%d ",arr[i][j]);
        }
        printf("\n");
    }
    
    
	return 0;
}


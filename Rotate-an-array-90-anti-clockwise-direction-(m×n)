#include<stdio.h>
int main() 
{
    int row,col;
    scanf("%d%d",&row,&col);
    int arr[row][col],arr1[col][row]; 
    
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col;j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }

    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col;j++)
        {
            arr1[j][i]=arr[i][j];

        }
    }
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col/2;j++)
        {
            int temp=arr1[j][i];
            arr1[j][i]=arr1[col-1-j][i];
            arr1[col-1-j][i]=temp;

        }
    }
    

    for(int i=0;i<col;i++)
    {
        for(int j=0;j<row;j++)
        {
            printf("%d ",arr1[i][j]);
        } 
        printf("\n");
    }

}

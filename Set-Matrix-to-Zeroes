#include<stdio.h>
int main() 
{
    int row,col,a,b;
    scanf("%d%d",&row,&col);
    int arr[row][col],arr1[col][row]; 
    
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col;j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col;j++)
        {
            if(arr[i][j]==0)
            {
                a=i;
                b=j;
            }
        }
    }

    for(int i=0;i<col;i++)
    {
        arr[a][i]=0;
    } 

    for(int i=0;i<row;i++)
    {
        arr[i][b]=0;
    }
    
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col;j++)
        {
            printf("%d ",arr[i][j]);
        }
        printf("\n");
    }
}
